<div id="create-user">
  <h3><%= button_to "Create New User", href: "/register", method: :get %></h3>>
</div>

<div id="existing-users">
  <h3>Existing Users</h3>
  <ul>
    <% @users.each do |user| %>
      <li><%= link_to "#{user.email}", href: "/users/#{user.id}", method: :get %></li>
    <% end %>
  </ul>
</div>